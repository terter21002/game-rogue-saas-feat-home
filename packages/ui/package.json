{
  "name": "@repo/ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    "./global.css": "./src/styles/global.css",
    "./lib/*": "./src/lib/*.ts",
    "./components/*": "./src/components/*.tsx",
    "./card": "./src/card.tsx",
    "./tailwind.config.ts": "./tailwind.config.ts",
    "./providers/*": "./src/providers/*.tsx",
    "./icons": "./src/icons/index.ts"
  },
  "scripts": {
    "build": "tailwindcss -i ./src/styles/global.css -o ./dist/index.css",
    "lint": "eslint src/",
    "dev": "tailwindcss -i ./src/styles/global.css -o ./dist/index.css --watch",
    "type-check": "tsc --noEmit",
    "ui:add": "npx shadcn-ui@latest add",
    "nextui:add": "npx nextui-cli@latest add",
    "nextui:remove": "npx nextui-cli@latest remove",
    "nextui:upgrade": "npx nextui-cli@latest upgrade",
    "clean": "rm -rf dist && rm -rf .turbo && rm -rf node_modules"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:^",
    "@repo/tailwind-config": "workspace:^",
    "@repo/typescript-config": "workspace:^",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@nextui-org/autocomplete": "^2.0.14",
    "@nextui-org/avatar": "^2.0.25",
    "@nextui-org/button": "^2.0.27",
    "@nextui-org/input": "^2.1.19",
    "@nextui-org/navbar": "^2.0.30",
    "@nextui-org/scroll-shadow": "^2.1.16",
    "@nextui-org/select": "^2.1.25",
    "@nextui-org/spacer": "^2.0.25",
    "@nextui-org/spinner": "^2.0.25",
    "@nextui-org/system": "^2.1.0",
    "@nextui-org/tabs": "^2.0.29",
    "@nextui-org/theme": "^2.2.0",
    "@nextui-org/tooltip": "^2.0.33",
    "@nextui-org/user": "^2.0.26",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@repo/types": "workspace:^",
    "@repo/utils": "workspace:^",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "cropperjs": "^1.6.2",
    "framer-motion": "^11.0.28",
    "lucide-react": "^0.368.0",
    "next-themes": "^0.3.0",
    "react-cropper": "^2.3.3",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  }
}
